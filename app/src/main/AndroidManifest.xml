<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.voteeverything">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".RateCompaniesActivity"></activity>
        <activity android:name=".ViewCompaniesActivity" />
        <activity android:name=".AddCompanyActivity" />
        <activity android:name=".UserActivity" />
        <activity android:name=".ViewVotesActivity" />
        <activity android:name=".VoteOnSurveyActivity" />
        <activity android:name=".ViewSurveysActivity" />
        <activity android:name=".CreateSurveyActivity" />
        <activity android:name=".MainWindowActivity" />
        <activity android:name=".StartScreenActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>